<%= form_for [@seller, @home, @address] do |f| %>
  <%= f.label :street %>
  <%= f.text_field :street %>
  <br />

  <%= f.label :city %>
  <%= f.text_field :city %>
  <br />

  <%= f.label :state %>
  <%= f.text_field :state %>
  <br />

  <%= f.label :zip %>
  <%= f.text_field :zip %>
  <br />

  <%= f.submit %>
<% end %>