<h1>Real Estate Portal</h1>
<hr />

<% if @sellers.any? %>
  <% @sellers.each do |seller| %>
    <ul>
    <li><%= link_to seller.name, seller_path(seller) %></li>
    </ul>
  <% end %>
<% else %>
  <h3>There's nothing to display</h3>
<% end %>

<hr />
<button type='button'>
<%= link_to "New Seller Profile", new_seller_path %>
</button>